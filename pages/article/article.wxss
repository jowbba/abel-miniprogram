.container {
  padding: 20px 5%;
}

.title {
  font-size: 20pt;
  text-align: center;
  margin-bottom: 11px;
}

.subTitle {
  font-size: 14pt;
  color: #888888;
  text-align: center;
  /* margin-bottom: 10px; */
}

.section-header {
  font-size: 17pt;
  color: #353535;
  margin-top: 40px;
  margin-bottom: 7px;
}

.paragraph {
  font-size: 13pt;
  line-height: 19pt;
  color: #353535;
  margin-bottom: 10px;
}

.image {
  width: 100%;
  margin-bottom: 10px;
}

.tip {
  box-sizing: border-box;
  box-shadow: 0px 1px 1px 0px rgba(0,0,0,0.2);
  padding: 5px 10px;
  font-size: 11pt;
  color: #b2b2b2;
  border: 1px solid #b2b2b2;
}

.remarks {
  font-size: 11pt;
  color: #b2b2b2;
  margin-bottom: 10px;
}

.highLight {
  font-size: 13pt;
  font-weight: 600;
  color: #353535;
  margin-bottom: 10px;
}