.container {
  width: 100%;
  /* height: 100%; */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
} 

.header {
  width: 100%;
  background: #009788;
  margin-bottom: 5px;
}

.header image {
  max-width: 100%;
  height: 100%;
  margin: 0 auto;
  display: block;
}

.content {
  display: flex;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
  /* padding: 0 10%; */
}

.colume {
  width: 100%;
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
}

.colume > navigator {
  width: 100%;
  height: 100px;
  padding:0 10%;
  box-sizing: border-box;
  color: #fff;
  position: relative;
}


.colume > navigator:nth-child(1) {
  background-color: #00695C; 
}

.colume > navigator:nth-child(2) {
  background-color: #80CBC4; 
}

.colume > navigator:nth-child(3) {
  background-color: #00796B;
}

.colume > navigator:nth-child(4) {
  background-color: #4DB6AC; 
}

.colume > navigator > text {
  position: absolute;
  bottom: 10px;
  font-size: 25px;
}

.colume > navigator:nth-child(2n) > text {
  right: 10px;
} 