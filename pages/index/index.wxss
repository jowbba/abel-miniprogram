.container {
  width: 100%;
  /* height: 100%; */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
} 



.header {
  width: 100%;
  background: #009788;
}

.header image {
  max-width: 100%;
  height: 100%;
  margin: 0 auto;
  display: block;
}

.content {
  display: flex;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
  padding: 0 10%;
}

.colume {
  width: 100%;
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
}

.colume > navigator {
  width: 100%;
  height: 50px;
  line-height: 50px;
}

.colume > navigator:nth-child(2n) {
  text-align: right;
}